{
  "name": "portfolio-eleventy",
  "version": "1.0.0",
  "description": "My portfolio based on TEA Stack.",
  "main": "index.js",
  "config": {
    "css_in": "src/_styles/_main.css",
    "css_out": "dist/assets/main.bundle.css",
    "js_in": "src/_scripts/_main.js",
    "js_out": "dist/assets/main.bundle.js"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all clean build:11ty build:css build:js",
    "build:11ty": "eleventy --quiet",
    "build:css": "npx @tailwindcss/cli -i $npm_package_config_css_in -o $npm_package_config_css_out --minify --postcss",
    "build:js": "node esbuild.config.js",
    "clean": "rm -rf dist && rm -rf .cache",
    "dev": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && cross-env NODE_ENV=development npm-run-all dev:11ty dev:css dev:js",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:css": "npx @tailwindcss/cli -i $npm_package_config_css_in -o $npm_package_config_css_out --watch --postcss",
    "dev:js": "node esbuild.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdevsuperfast/portfolio-eleventy.git"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "htmlWhitespaceSensitivity": "ignore"
  },
  "keywords": [
    "tailwind",
    "eleventy",
    "alpine"
  ],
  "author": "Rotsen Mark Acob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdevsuperfast/portfolio-eleventy/issues"
  },
  "homepage": "https://github.com/webdevsuperfast/portfolio-eleventy#readme",
  "dependencies": {
    "@11ty/eleventy-img": "^6.0.1",
    "acclaimed": "^1.0.12",
    "alpinejs": "^3.14.8",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "axios-cache-interceptor": "^1.6.2",
    "cssnano": "^7.0.6",
    "eleventy-critical-css": "^2.0.2",
    "gsap": "^3.12.7",
    "https": "^1.0.0",
    "path": "^0.12.7",
    "postcss": "^8.5.1",
    "postcss-nested-ancestors": "^3.0.0",
    "vanilla-lazyload": "^19.1.3",
    "venobox": "^2.1.8"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "esbuild": "^0.24.2",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^16.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.3",
    "wrangler": "^3.107.2"
  },
  "type": "module"
}
