[
  {
    "css": "https://www.rotsenacob.com/assets/main.bundle.css",
    "out": "critical.min.css",
    "url": "https://www.rotsenacob.com/",
    "width": 1920,
    "height": 1800,
    "keepLargerMediaQueries": true,
    "strict": false,
    "blockJSRequests": true,
    "renderWaitTime": 300,
    "timeout": 60000,
    "forceExclude": ["@font-face"],
    "forceInclude": [".dark", ".dark .inner:not([id*=\"kss\"])", ".dark img"],
    "phantomJsOptions": {
      "ssl-protocol": "any",
      "ignore-ssl-errors": true
    }
  }
]
